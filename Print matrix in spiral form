        }
        if(left < right) {
            for(int i = bottom-1; i >= top; i--) printf("%d ", a[i][left]);
            left++;
        }
    }
}
int main() {
    int a[10][10], row = 3, col = 3;
    for(int i=0; i<row; i++)
        for(int j=0; j<col; j++)
            scanf("%d", &a[i][j]);
    spiral(a, row, col);
    return 0;
}

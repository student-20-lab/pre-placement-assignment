int main() {
    int mat[100][100], r, c, count = 0;
    scanf("%d %d", &r, &c);
    for (int i = 0; i < r; i++)
        for (int j = 0; j < c; j++) {
            scanf("%d", &mat[i][j]);
            if (mat[i][j] == 0) count++;
        }
    printf("%d", count);
    return 0;

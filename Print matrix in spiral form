#include <stdio.h>
void spiral(int a[10][10], int row, int col) {
    int top = 0, bottom = row, left = 0, right = col;
    while(top < bottom && left < right) {
        for(int i = left; i < right; i++) printf("%d ", a[top][i]);
        top++;
        for(int i = top; i < bottom; i++) printf("%d ", a[i][right-1]);
        right--;
        if(top < bottom) {

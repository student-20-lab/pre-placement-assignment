#include <stdio.h>

int main() {
    int a[100], b[100], res[100], n;
    scanf("%d", &n); 
    for (int i = 0; i < n; i++) scanf("%d", &a[i]);
    for (int i = 0; i < n; i++) scanf("%d", &b[i]);
    for (int i = 0; i < n; i++) res[i] = a[i] + b[i];
    for (int i = 0; i < n; i++) printf("%d ", res[i]);
    return 0;
}

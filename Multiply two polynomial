#include <stdio.h>

int main() {
    int a[100], b[100], res[200] = {0}, n1, n2;
    scanf("%d", &n1);
    for (int i = 0; i < n1; i++) scanf("%d", &a[i]);
    scanf("%d", &n2);
    for (int i = 0; i < n2; i++) scanf("%d", &b[i]);
    for (int i = 0; i < n1; i++)
        for (int j = 0; j < n2; j++)
            res[i + j] += a[i] * b[j];
    for (int i = 0; i < n1 + n2 - 1; i++) printf("%d ", res[i]);
    return 0;
}

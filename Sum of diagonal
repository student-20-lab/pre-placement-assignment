#include <stdio.h>

int main() {
    int n, mat[100][100], sum = 0;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++) scanf("%d", &mat[i][j]);

    for (int i = 0; i < n; i++) {
        sum += mat[i][i];
        if (i != n - i - 1) sum += mat[i][n - i - 1];
    }
    printf("%d", sum);
    return 0;
}


#include <stdio.h>
int main() {
    int n, arr[100], j = 0;
    scanf("%d", &n);
    int result[100];
    for(int i = 0; i < n; i++) scanf("%d", &arr[i]);
    for(int i = 0; i < n; i++) if(arr[i] != 0) result[j++] = arr[i];
    while(j < n) result[j++] = 0;
    for(int i = 0; i < n; i++) printf("%d ", result[i]);
    return 0;
}

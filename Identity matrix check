#include <stdio.h>

int main() {
    int n, mat[100][100], flag = 1;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++) {
            scanf("%d", &mat[i][j]);
            if ((i == j && mat[i][j] != 1) || (i != j && mat[i][j] != 0))
                flag = 0;
        }

    if (flag)
        printf("Identity matrix");
    else
        printf("Not an identity matrix");

    return 0;
}


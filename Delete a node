struct Node* deleteNode(struct Node* head, int position) {
    if (position == 0) {
        struct Node* temp = head;
        head = head->next;
        free(temp);
        return head;
    }
    struct Node* temp = head;
    for (int i = 0; i < position - 1 && temp; i++)
        temp = temp->next;
    if (temp && temp->next) {
        struct Node* del = temp->next;
        temp->next = del->next;
        free(del);
    }
    return head;
}

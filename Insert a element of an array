#include <stdio.h>
int main() {
    int arr[100], n, x;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    printf("Enter %d elements: ", n);
    for(int i=0; i<n; i++) scanf("%d", &arr[i]);
    printf("Enter element to insert: ");
    scanf("%d", &x);
    arr[n] = x;
    n++;
    printf("Array after insertion: ");
    for(int i=0; i<n; i++) printf("%d ", arr[i]);
    return 0;
}
